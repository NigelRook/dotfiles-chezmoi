autoload -Uz compinit
compinit
zstyle ':completion:*' menu select

export PATH="$PATH:$HOME/bin"

if which starship >> /dev/null 2>&1 ; then
  eval "$(starship init zsh)"
fi

if [[ -f "$HOME/.atuin/bin/env" ]]; then
  . "$HOME/.atuin/bin/env"
fi

if which atuin >> /dev/null 2&>1; then
  eval "$(atuin init zsh)"
fi


